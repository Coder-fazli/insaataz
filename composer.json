{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-debugbar": "^3.7",
        "bumbummen99/shoppingcart": "^4.2",
        "ebess/advanced-nova-media-library": "^4.0",
        "emilianotisato/nova-tinymce": "^2.0",
        "epartment/nova-dependency-container": "^1.3",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^9.19",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "mcamara/laravel-localization": "^1.7",
        "mhaciyev/nova-dependency-container": "*",
        "mostafaznv/nova-ckeditor": "^3.1",
        "optimistdigital/nova-translatable": "^2.0",
        "orlyapps/nova-multiline-text": "^0.0.1",
        "ostheneo/nova-fields": "^1.1",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-image-optimizer": "*",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-translation-loader": "^2.7",
        "whitecube/nova-flexible-content": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "functions.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "path",
            "url": "./nova"
        },
        "0": {
            "type": "path",
            "url": "./nova-components/NovaDependencyContainer"
        }
    }
}
